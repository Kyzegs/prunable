{
    "name": "kyzegs/prunable",
    "description": "Laravel's prunable traits without force deleting",
    "authors": [
        {
            "name": "Sebastiaan \"Kyzegs\" Zegers",
            "email": "kyzegs@gmail.com"
        }
    ],
    "require": {
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/events": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Kyzegs\\Prunable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kyzegs\\Prunable\\Providers\\PrunableProvider"
            ]
        }
    }
}
